{
  "name": "golf-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "start": "serve -s dist -l 8080",
    "check:deps": "depcheck",
    "check:updates": "ncu",
    "check:security": "npm audit",
    "check:all": "npm run check:security && npm run check:deps && npm run check:updates",
    "update:deps": "ncu -u && npm install",
    "fix:audit": "npm audit fix",
    "ci": "node scripts/pre-push-check.js",
    "ci:check": "node scripts/pre-push-check.js",
    "ci:strict": "node scripts/validate-ci-strict.js",
    "ci:validate": "node scripts/pre-push-check.js",
    "pre-push": "node scripts/pre-push-check.js"
  },
  "dependencies": {
    "@capacitor/app": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/status-bar": "^7.0.2",
    "@ionic/react": "^8.7.3",
    "@ionic/react-router": "^8.7.3",
    "@supabase/supabase-js": "^2.56.0",
    "date-fns": "^4.1.0",
    "ionicons": "^8.0.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^5.3.4",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "npm-check-updates": "^18.0.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
